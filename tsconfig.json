{
  "compilerOptions": {
    "target": "es5",
    "removeComments": true,    
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["es2018", "dom", "webworker"],
    "module": "commonjs",
    "esModuleInterop": true,    
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "jsx": "react",
    "jsxFactory": "Snabbdom.createElement"
  },
  "include": ["src"]
}
